#include <iostream>
#include <string>
using namespace std;

void main ()
{
	string nama; //pemanggil nama
	nama = "Abdullah"; 
	
	int usia; //pemanggil tinggi
	usia = 64;
	
	float tinggi; //pemanggil tinggi
	tinggi = 17.64;
	
	string hobby; //pemanggil hobby
	hobby = " main bola, nulis, ngoding";

	cout <<"nama = " <<nama<<endl;
	cout <<"usia = " <<usia<<endl;
	cout <<"tinggi = " <<tinggi<<endl;
	cout <<"hobby = " <<hobby<<endl;
	
}
